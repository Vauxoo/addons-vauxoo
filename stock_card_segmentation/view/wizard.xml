<?xml version='1.0' encoding='utf-8'?>
<openerp>
    <data>
        <record id="stock_card_move_tree" model="ir.ui.view">
            <field name="name">stock.card.move.tree.segmentation</field>
            <field name="model">stock.card.move</field>
            <field name="inherit_id" ref="stock_card.stock_card_move_tree"/>
            <field name="arch" type="xml">
                <xpath expr='//field[@name="average"]' position='after'>
                    <field name="material"/>
                    <field name="landed"/>
                    <field name="production"/>
                    <field name="subcontracting"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
