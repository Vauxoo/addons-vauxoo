  <story>
    <para style="P2">BALANZA DE COMPROBACION</para>
    <para style="P10">
      <font color="white"> </font>
    </para>
    <para style="P8">Fecha Inicial: [[ time.strftime('%d-%m-%Y',time.strptime( data['form'][0]['date_ini'] ,'%Y-%m-%d')) ]]</para>
    <para style="P8">Fecha Final: [[ time.strftime('%d-%m-%Y',time.strptime( data['form'][0]['date_fin'] ,'%Y-%m-%d')) ]]</para>
    <para style="P8">Nivel: [[ data['form'][0]['nivel'] ]]</para>
    <blockTable colWidths="83.0,104.0,74.0,82.0,73.0,101.0" style="Tabla2">
      <tr>
        <td>
          <para style="P5">Codigo</para>
        </td>
        <td>
          <para style="P5">Cuenta</para>
        </td>
        <td>
          <para style="P5">Saldo inicial</para>
        </td>
        <td>
          <para style="P5">debe</para>
        </td>
        <td>
          <para style="P5">haber</para>
        </td>
