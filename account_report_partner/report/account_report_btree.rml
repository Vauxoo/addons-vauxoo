    </blockTable>
    <para style="P1">
      <font color="white"> </font>
    </para>
    <section>
      <para style="P4">[[ repeatIn(get_account_moves(data['form']), 'p') ]]</para>
      <blockTable colWidths="84.0,102.0,74.0,82.0,74.0,101.0" style="Tabla1">
        <tr>
          <td>
            <para style="P6">[[ p['code'] ]]</para>
          </td>
          <td>
            <para style="P7">[[ p['name'] ]]</para>
          </td>
          <td>
            <para style="P6">[[formatLang(p['saldo_inicial'])]]</para>
          </td>
          <td>
            <para style="P6">[[formatLang(p['debit'])]]</para>
          </td>
          <td>
            <para style="P6">[[formatLang(p['credit'])]]</para>
          </td>
          <td>
            <para style="P6">[[formatLang(p['saldo_final'])]]</para>
          </td>
        </tr>
      </blockTable>
      <para style="P3">
        <font color="white"> </font>
      </para>
    </section>
    <para style="Standard">
      <font color="white"> </font>
