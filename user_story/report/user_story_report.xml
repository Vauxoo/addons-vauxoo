<openerp>
	<data>
		<report
		    id="user_story_report"
		    model="user.story"
		    string="User stories"
		    report_type="qweb-pdf"
		    name="user_story.user_story_report_template"
		    file="user_story_report"
		    menu="True"
		    attachment="'user_story_'+object.name+'.pdf'"
		/>

		<template id="user_story_report_template">
		    <t t-call="report.html_container">
		        <t t-foreach="docs" t-as="o">
		            <t t-call="report.external_layout">
		                <div class="page">
		                    <h2>User story report</h2>
		                    <p> To be migrated!</p>
		                </div>
		            </t>
		        </t>
		    </t>
		</template>


	</data>



</openerp>