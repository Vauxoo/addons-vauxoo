  <story>
    <section>
      <para style="P19">[[ repeatIn(objects,'o') ]] <font face="Helvetica" size="9.0">[[ set_global_data(o) ]]</font></para>
      <para style="P22">[[ o.company_id.parent_id and o.company_id.parent_id.partner_id.name or o.company_id.partner_id.name ]]</para>
      <blockTable colWidths="125.0,207.0,166.0" style="Tabla1">
        <tr>
          <td>
            <para style="P27">[[ o.company_id.logo and setTag('para','image',{'width':'120.0','height':'80.0'}) ]] [[ o.company_id.logo ]]</para>
          </td>
          <td>
            <para style="P21">[[ company_address()['name'] or o.company_id.partner_id.name ]]</para>
            <para style="P24">
              <font color="white"> </font>
            </para>
            <para style="P21">Calle: [[ company_address()['street'] or '' ]] Nro. Ext: [[ company_address()['street3'] or '' ]] Int: [[ company_address()['street4'] or '' ]]</para>
            <para style="P24">
              <font color="white"> </font>
            </para>
            <para style="P21">Colonia: [[ company_address()['street2'] or '' ]]</para>
            <para style="P24">
              <font color="white"> </font>
            </para>
            <para style="P21">Ciudad: [[ company_address()['city'] or '' ]] Estado: [[ company_address()['state_id'] and company_address()['state_id']['name'] or '' ]]</para>
            <para style="P25">
              <font color="white"> </font>
            </para>
            <para style="P21">Localidad: [[ company_address()['city2'] or '' ]]</para>
            <para style="P24">
              <font color="white"> </font>
            </para>
            <para style="P21">CP: [[ company_address()['zip'] or '' ]]</para>
            <para style="P24">
              <font color="white"> </font>
            </para>
            <para style="P21">RFC: [[ o.company_id.partner_id._columns.has_key('vat_split') and o.company_id.partner_id.vat_split or o.company_id.partner_id.vat ]]</para>
            <para style="P24">
              <font color="white"> </font>
            </para>
            <para style="P23">Teléfono(s): </para>
            <para style="P23">[[ company_address()['phone'] or removeParentNode('para') ]]</para>
            <para style="P23">[[ company_address()['fax'] or removeParentNode('para') ]]</para>
            <para style="P23">[[ company_address()['mobile'] or removeParentNode('para') ]]</para>
          </td>
          <td>
            <para style="P7">
              <font face="Helvetica" size="18.0">Folio: [[ ( o.type in ['out_invoice', 'out_refund'] ) and ( o.state in ['open', 'paid', 'cancel'] ) and o.number or 'SIN FOLIO O ESTATUS NO VALIDO' ]]</font>
            </para>
            <para style="P9">
              <font color="white"> </font>
            </para>
            <para style="P8">[[ o.state == 'cancel' and 'FACTURA CANCELADA' ]]</para>
            <para style="P9">
              <font color="white"> </font>
            </para>
            <para style="Table Contents">[[ company_address()['city'] or '' ]], [[ company_address()['state_id'] and company_address()['state_id']['name'] or '' ]] a</para>
            <para style="P28">[[ o.date_invoice_tz or '' ]]</para>
            <para style="Table Contents">Serie: [[ get_approval() and get_approval().serie or '' ]]</para>
            <para style="Table Contents">Aprobación: [[ get_approval() and get_approval().approval_number or '' ]]</para>
            <para style="Table Contents">Año Aprobación:[[ get_approval() and get_approval().approval_year or '' ]]</para>
          </td>
        </tr>
      </blockTable>
      <blockTable colWidths="249.0,258.0" style="Tabla2">
        <tr>
          <td>
            <para style="P5">Receptor</para>
            <para style="Table Contents"><font face="Helvetica">Nombre: </font>[[ o.partner_id.name ]]</para>
            <para style="terp_default_8"><font face="Helvetica">Dirección:</font> [[ o.address_invoice_id.street ]] No. Ext: [[ o.address_invoice_id.street3 ]] Int: [[ o.address_invoice_id.street4 ]]</para>
            <para style="terp_default_8"><font face="Helvetica">Colonia:</font> [[ o.address_invoice_id.street2 or '' ]]</para>
            <para style="terp_default_8"><font face="Helvetica">Ciudad:</font> [[ o.address_invoice_id.city or '' ]] <font face="Helvetica">Estado: </font>[[ o.address_invoice_id.state_id and o.address_invoice_id.state_id.name or '' ]]</para>
            <para style="terp_default_8"><font face="Helvetica">Localidad: </font>[[ o.address_invoice_id.city2 or '' ]]</para>
            <para style="terp_default_8"><font face="Helvetica">C.P.:</font> [[ o.address_invoice_id.zip or '' ]] </para>
            <para style="terp_default_8"><font face="Helvetica">R. F. C. :</font> [[ o.partner_id._columns.has_key('vat_split') and o.partner_id.vat_split or o.partner_id.vat ]]</para>
            <para style="P26">Teléfono(s): </para>
            <para style="P20">[[ o.address_invoice_id.phone or removeParentNode('para') ]]</para>
            <para style="P20">[[ o.address_invoice_id.fax or removeParentNode('para') ]]</para>
            <para style="P20">[[ o.address_invoice_id.mobile or removeParentNode('para') ]]</para>
          </td>
          <td>
            <para style="Table Contents">Condición de pago: [[ format(o.payment_term and (o.payment_term.note or o.payment_term.name) or removeParentNode('para') ) ]]</para>
            <para style="P3">Origen: [[ o.origin or removeParentNode('para') ]]</para>
            <para style="P3">Expedido en:</para>
            <para style="P3">[[ subcompany_address()['name'] or o.company_id.partner_id.name ]]</para>
            <para style="P3">Calle: [[ subcompany_address()['street'] or '' ]] Nro. Ext: [[ subcompany_address()['street3'] or '' ]] Int: [[ subcompany_address()['street4'] or '' ]]</para>
            <para style="P3">Colonia: [[ subcompany_address()['street2'] or '' ]]</para>
            <para style="P3">Ciudad: [[ subcompany_address()['city'] or '' ]] </para>
            <para style="P3">Estado: [[ subcompany_address()['state_id'] and subcompany_address()['state_id']['name'] or '' ]]</para>
            <para style="P3">Localidad: [[ subcompany_address()['city2'] or '' ]]</para>
            <para style="P3">CP: [[ subcompany_address()['zip'] or '' ]] </para>
          </td>
        </tr>
      </blockTable>
      <blockTable colWidths="55.0,43.0,158.0,55.0,63.0,54.0,81.0" style="Tabla3">
        <tr>
          <td>
            <para style="P5">Cant.</para>
          </td>
          <td>
            <para style="P5">Unidad</para>
          </td>
          <td>
            <para style="P5">Descripción</para>
          </td>
          <td>
            <para style="P5">Clave</para>
          </td>
          <td>
            <para style="P6">P.Unitario</para>
          </td>
          <td>
            <para style="P6">Dto. %</para>
          </td>
          <td>
            <para style="P6">Importe</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="Table Contents"><font face="Helvetica" size="8.0">[[ repeatIn(o.invoice_line,'l') ]] </font>[[ formatLang(l.quantity) ]]</para>
          </td>
          <td>
            <para style="Table Contents">[[ (l.uos_id and l.uos_id.name) or '' ]]</para>
          </td>
          <td>
            <para style="Table Contents">[[ l.name ]]</para>
            <para style="P4">
              <font face="Helvetica" size="7.0">Notas:</font>
              <font face="Helvetica" size="7.0">[[l.note or removeParentNode('para')]]</font>
            </para>
          </td>
          <td>
            <para style="Table Contents">[[ l.product_id and l.product_id.default_code ]]</para>
          </td>
          <td>
            <para style="P1">[[ formatLang(l.price_unit) ]]</para>
          </td>
          <td>
            <para style="P1">[[ formatLang(l.discount) ]]<font face="Helvetica" size="11.0">%</font></para>
          </td>
          <td>
            <para style="P1">[[ exists_key('global_discount_percent') and (formatLang(l.quantity * l.price_unit, digits=get_digits(dp='Account'))) or formatLang(l.price_subtotal) ]]</para>
          </td>
        </tr>
      </blockTable>
      <blockTable colWidths="414.0,93.0" style="Tabla7">
        <tr>
          <td>
            <para style="P6">Suma $</para>
          </td>
          <td>
            <para style="P1">[[formatLang( ( exists_key('global_discount_percent') and o.global_discount_amount or 0.0 ) + (o.amount_untaxed or 0.0), digits=get_digits(dp='Account') )]]</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P6">Descuento: [[ exists_key('global_discount_percent') and o.global_discount_percent or removeParentNode('tr')]] %</para>
          </td>
          <td>
            <para style="P1">[[o.global_discount_amount and formatLang( o.global_discount_amount) or '']]</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P6">Sub Total $</para>
          </td>
          <td>
            <para style="P1">[[ formatLang(o.amount_untaxed) ]]</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P6">
              <font face="Helvetica" size="6.0">[[ repeatIn( get_taxes(), 'tax' ) ]]</font>
              <font face="Helvetica">[[ tax['name2'] ]] ([[ round( float( tax['tax_percent'] ) ) ]]%) $</font>
            </para>
          </td>
          <td>
            <para style="P1">[[ formatLang( float( tax['amount'] ) ) ]]</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P6">
              <font face="Helvetica" size="6.0">[[ repeatIn( get_taxes_ret(), 'tax_ret' ) ]]</font>
              <font face="Helvetica">[[ tax_ret['name2'] ]] Ret ([[ round( float( tax_ret['tax_percent'] ),2 )*-1 ]]%) $</font>
            </para>
          </td>
          <td>
            <para style="P1">[[ formatLang( float( tax_ret['amount'] )*-1 ) ]]</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P6">Total $</para>
          </td>
          <td>
            <para style="P1">[[ formatLang(o.amount_total) ]]</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P16">IMPORTE CON LETRA:</para>
            <para style="P14">[[ o.amount_to_text ]]</para>
          </td>
          <td>
            <para style="P1">
              <font color="white"> </font>
            </para>
          </td>
        </tr>
      </blockTable>
      <para style="P13">PAGO EN UNA SOLA EXHIBICIÓN</para>
      <para style="P13">
        <font color="white"> </font>
      </para>
      <para style="P13">“Este documento es una representacion impresa de un CFD” </para>
      <para style="P13">CFD, Comprobante Fiscal Digital</para>
      <para style="P18">
        <font color="white"> </font>
      </para>
      <blockTable colWidths="157.0,175.0,166.0" style="Tabla5">
        <tr>
          <td>
            <para style="P10">Régimen Fiscal </para>
            <para style="P11">[[o.company_id.partner_id.regimen_fiscal_id and o.company_id.partner_id.regimen_fiscal_id.name or 'No identificado']]</para>
          </td>
          <td>
            <para style="P10">Método de Pago: </para>
            <para style="P12">[[o.pay_method_id.name or 'No identificado']]</para>
          </td>
          <td>
            <para style="P10">Últimos 4 dígitos de la cuenta bancaria:</para>
            <para style="P12">[[o.acc_payment.last_acc_number or 'No identificado']]</para>
          </td>
        </tr>
      </blockTable>
      <para style="P29">
        <font color="white"> </font>
      </para>
      <blockTable colWidths="101.0,397.0" style="Tabla4">
        <tr>
          <td>
            <para style="P2">[[ o.company_id.cif_file and setTag('para','image',{'width':'98.0','height':'161.0'}) ]] [[ o.company_id.cif_file ]]</para>
          </td>
          <td>
            <para style="P13">Cadena original :</para>
            <para style="P13">[[ split_string( o.cadena_original ) ]]</para>
            <para style="P13">
              <font color="white"> </font>
            </para>
            <para style="P13">Serie del Certificado :</para>
            <para style="P13">[[ o.no_certificado or '' ]]</para>
            <para style="P13">
              <font color="white"> </font>
            </para>
            <para style="P13">Sello digital :</para>
            <para style="P15">[[ split_string( o.sello ) ]]</para>
          </td>
        </tr>
      </blockTable>
      <para style="P17">Vendedor: [[o.user_id.name]]</para>
      <para style="terp_default_9">[[ format(o.comment or '') ]]</para>
    </section>
    <para style="Standard">
      <font color="white"> </font>
    </para>
  </story>
</document>

