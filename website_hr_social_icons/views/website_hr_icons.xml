<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<!-- Page --> 

<template id="aboutus_social" inherit_id="website_hr.aboutus" name="Our Team Social">
    <xpath expr="//div[@t-field='employee.public_info']" position="replace">
                <div class="bottom social_footer">
                    <a t-att-href="employee.twitter">
                        <i id="social" class="fa fa-twitter-square fa-2x social-tw"></i>
                    </a>
                    <a rel="publisher"
                       t-att-href="employee.github">
                        <i id="social" class="fa fa-github-square fa-2x social-fb"></i>
                    </a>
                </div>
    </xpath>
</template>


 </data>
</openerp>
